{
  "name": "shop-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest",
    "compile": "tsc",
    "offline": "sls offline --noPrependStageInUrl",
    "log": "serverless logs -f <functionName>",
    "invoke:getProductsList": "sls invoke local --function getProductsList",
    "invoke:getProductsById": "sls invoke local --function getProductsById --data `{ \"pathParameters\": { \"productId\": \"02219ff1-5675-4bb7-8af3-9077bf338c29\" } }`",
    "deploy": "sls deploy"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "dotenv": "^16.0.1",
    "pg": "^8.7.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^28.1.1",
    "@types/node": "^14.14.25",
    "@types/pg": "^8.6.5",
    "@types/uuid": "^8.3.4",
    "esbuild": "^0.14.11",
    "jest": "^28.1.1",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.5.1",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^8.8.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
