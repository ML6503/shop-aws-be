{
    "name": "bff-aws",
    "version": "1.0.0",
    "description": "Backend for frontend service",
    "main": "index.ts",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/index.js",
        "dev": "npx ts-node src/index.ts",
        "dev:watch": "nodemon src/index.ts",
        "eb-init": "eb init --platform node.js --region eu-west-1",
        "eb-create": "eb create development --single --cname ML6503-bff-api-dev",
        "eb-terminate": "eb terminate"
    },
    "keywords": [
        "express",
        "node.js",
        "elastik",
        "beanstalk"
    ],
    "author": "ML6503",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.27.2",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "http-status": "^1.5.2",
        "node-cache": "^5.1.2"
    },
    "engines": {
        "npm": ">=8.5.5",
        "node": ">=16.15.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^18.6.3",
        "nodemon": "^2.0.19",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}
